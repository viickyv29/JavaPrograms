public class String1 {

	public static void main(String[] args) {
		String name = "racecar";
		char[] temp1 = name.toCharArray();
		System.out.println(temp1.length);
		for (int i=0;i<temp1.length;i++){
			for (int j=i+1;j<temp1.length;j++){
				if(temp1[i]==temp1[j]){
					System.out.println(temp1[i]);
				}
			}
		}
	}
}
